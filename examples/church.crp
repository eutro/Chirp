defn zero(f) = fn(x) = x

defn one(f) = fn(x) = f(x)

defn two(f) = fn(x) = f(f(x))

defn three(f) = fn(x) = f(f(f(x)))

defn add(a, b) = \f.\x.a(f)(b(f)(x))

defn mul(a, b) = \f.\x.a(b(f))(x)

defn pow(a, b) = \f.\x.a(b)(f)(x)

defn church2int(n) = n(\x.x + 1)(0)

church2int(mul(two, pow(three, three)))
